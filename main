using System;
using System.Drawing;
using System.Globalization;
using System.Threading;

namespace Hex
{
    class Cl1
    {
        static void Main()
        {
            int value = 4537;
            int toBase = 16;
            string hex = Convert.ToString(value, toBase);
            string hexU = hex.ToUpper();
            Console.WriteLine(hexU);
        }
    }
}
